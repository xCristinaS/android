<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="c.ejercicio46_firebasealumnos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".actividades.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actividades.NotasActivity"
            android:label="@string/title_activity_notas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".actividades.GruposActivity"
            android:label="@string/title_activity_grupos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
